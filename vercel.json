{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index"
        },
        {
            "source": "/2",
            "destination": "/api/two"
        },
        {
            "source": "/contact",
            "destination": "/api/contact"
        }
    ]
}